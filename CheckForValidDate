CREATE OR REPLACE FUNCTION "FUNCTIONS"."f_IS_DATE" (
  "DT" text
)
RETURNS boolean AS
$body$
BEGIN
	PERFORM $1::DATE;
    RETURN TRUE;
    
    EXCEPTION WHEN others THEN
    RETURN FALSE;
END;
$body$
LANGUAGE 'plpgsql'
VOLATILE
CALLED ON NULL INPUT
SECURITY INVOKER
PARALLEL UNSAFE
COST 100;

ALTER FUNCTION "FUNCTIONS"."f_IS_DATE" ("DT" text)
  OWNER TO root;
